{
  "name": "modus",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "axios": "^0.9.1",
    "gregorian-calendar": "^4.1.3",
    "moment": "^2.12.0",
    "moment-duration-format": "^1.3.0",
    "rc-time-picker": "^1.1.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-maskedinput": "^3.1.2",
    "react-select": "0.9.1",
    "react-time-picker": "^1.1.0"
  },
  "devDependencies": {
    "babel-core": "6.7.6",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "0.23.1",
    "dotenv": "2.0.0",
    "eslint": "^2.5.3",
    "eslint-config-codeyellow": "0.2.3",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.15.0",
    "lodash": "^4.10.0",
    "style-loader": "^0.13.1",
    "webpack": "1.12.15",
    "webpack-dev-server": "1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "engines": {
    "node": ">=4.2.3",
    "npm": ">=2.14.7"
  },
  "scripts": {
    "lint": "eslint *.js src && echo 'Linting succeeded.'",
    "build": "npm run -s lint && npm run -s clean && npm run -s build-js",
    "build-js": "webpack --colors --progress --bail",
    "clean": "rm -rf dist/*",
    "start": ". ../.env; webpack-dev-server --output-public-path / --port $MODUS_WEBPACK_PORT --host $MODUS_WEBPACK_HOST"
  }
}
